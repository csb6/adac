WITH REPORT; USE REPORT;
PRAGMA ELABORATE (REPORT);

PACKAGE LA3004B5 IS

     I : INTEGER := IDENT_INT (5);

     TASK LA3004B5T IS
          ENTRY E (I : IN OUT INTEGER);
     END LA3004B5T;

END LA3004B5;


PACKAGE BODY LA3004B5 IS

     TASK BODY LA3004B5T IS SEPARATE;

BEGIN

     LA3004B5T.E (I);

END LA3004B5;


WITH LA3004B0;
PRAGMA ELABORATE (LA3004B0);

SEPARATE (LA3004B5)
TASK BODY LA3004B5T IS

     J : INTEGER;

BEGIN

     ACCEPT E (I : IN OUT INTEGER) DO
          J := I;
          J := LA3004B0 + J;
          I := J;
     END E;

END LA3004B5T;
