WITH REPORT; USE REPORT;
PRAGMA ELABORATE (REPORT);

PACKAGE LA3004A4 IS
     
     I : INTEGER := IDENT_INT (4);
     FUNCTION F (I : INTEGER) RETURN INTEGER;  -- TO REQUIRE A PKG BODY.

END LA3004A4;


WITH LA3004A0;
PRAGMA ELABORATE (LA3004A0);

PACKAGE BODY LA3004A4 IS

     FUNCTION F (I : INTEGER) RETURN INTEGER IS
     BEGIN
          RETURN IDENT_INT (I);
     END F;

BEGIN

     I := F (I);
     LA3004A0.P (I);

END LA3004A4;
