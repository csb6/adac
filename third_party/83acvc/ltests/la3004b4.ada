-- LA3004B4.ADA

-- WKB 07/13/81
-- SPS 11/09/82
-- JRK 03/26/84
-- EG  06/05/84
-- JRK 5/21/85


WITH REPORT; USE REPORT;
PRAGMA ELABORATE (REPORT);

PACKAGE LA3004B4 IS

     I : INTEGER := IDENT_INT (4);

     PACKAGE LA3004B4Q IS
          FUNCTION F (I : INTEGER) RETURN INTEGER;  -- REQUIRE PKG BODY.
     END LA3004B4Q;

END LA3004B4;


PACKAGE BODY LA3004B4 IS

     PACKAGE BODY LA3004B4Q IS SEPARATE;

BEGIN

     I := I + 2;

END LA3004B4;


WITH LA3004B0;
PRAGMA ELABORATE (LA3004B0);

SEPARATE (LA3004B4)
PACKAGE BODY LA3004B4Q IS

     FUNCTION F (I : INTEGER) RETURN INTEGER IS
     BEGIN
          RETURN IDENT_INT (I);
     END F;

BEGIN

     I := F (I);
     I := LA3004B0 + I;

END LA3004B4Q;
