PROCEDURE LA3004B2 (I : OUT INTEGER) IS

     PROCEDURE LA3004B2P (NEW_I : OUT INTEGER) IS SEPARATE;

BEGIN

     LA3004B2P (I);

END LA3004B2;


WITH REPORT; USE REPORT;
WITH LA3004B0;

SEPARATE (LA3004B2)
PROCEDURE LA3004B2P (NEW_I : OUT INTEGER) IS

     I : INTEGER := IDENT_INT (2);

BEGIN

     I := LA3004B0 + I;
     NEW_I := I;

END LA3004B2P;
