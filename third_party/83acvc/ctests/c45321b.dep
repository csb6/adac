-- C45321B.DEP

-- OBJECTIVE:
--     TESTS ON ADDING OPERATORS AS 12 SUBTESTS.
--       (A) THE OPERATOR +, A, B, AND A+B ALL MODEL NUMBERS.
--       (B) THE OPERATOR +, A, B MODEL NUMBERS A+B NOT.
--       (C) THE OPERATOR +, A MODEL NUMBER, B, A+B NOT.
--       (D) THE OPERATOR +, A, B, AND A+B ALL MODEL NUMBERS OF
--           DIFFERENT SUBTYPES.
--       (E) THE OPERATOR +, A, B MODEL NUMBERS A+B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (F) THE OPERATOR +, A MODEL NUMBER, B, A+B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (G) THE OPERATOR -, A, B, AND A-B ALL MODEL NUMBERS.
--       (H) THE OPERATOR -, A, B MODEL NUMBERS A-B NOT.
--       (I) THE OPERATOR -, A MODEL NUMBER, B, A-B NOT.
--       (J) THE OPERATOR -, A, B, AND A-B ALL MODEL NUMBERS OF
--           DIFFERENT SUBTYPES.
--       (K) THE OPERATOR -, A, B MODEL NUMBERS A-B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (L) THE OPERATOR -, A MODEL NUMBER, B, A-B NOT, WITH
--           DIFFERENT SUBTYPES.
--      THIS TEST IS TO BE REPEATED FOR 'DIGITS = 5 .. 29.

-- APPLICABILITY CRITERIA:
--     THIS TEST IS APPLICABLE ONLY TO IMPLEMENTATIONS WHICH SUPPORT A
--     VALUE OF DIGITS GREATER THAN OR EQUAL TO 6.

--     IF 'DIGITS 6' IS NOT SUPPORTED, THEN THE DECLARATION OF 'TYPE F'
--     MUST BE REJECTED.

-- HISTORY:
--     BAW 09/17/80
--     JBG 11/10/82
--     JBG 07/29/84
--     JBG 10/12/86 ADD CODE TO ENSURE RUN-TIME ADDING OPERATORS ARE
--              USED BY OPTIMIZING COMPILERS; REMOVE -B FROM TEST NAME.
--     DHH 01/11/88 ADDED APPLICABILITY CRITERIA AND STANDARD HEADER.

WITH REPORT;
PROCEDURE C45321B IS

     USE REPORT;

     N : CONSTANT INTEGER := 6;      -- NUMBER OF DIGITS
     TYPE F IS DIGITS N;             -- MANTISSA BITS: 21  NA => ERROR.
     SUBTYPE F1 IS F DIGITS N-1;     -- MANTISSA BITS: 18
     SUBTYPE F2 IS F DIGITS N-2;     -- MANTISSA BITS: 15
BEGIN TEST ("C45321B", "CHECK FLOATING POINT ADDING OPERATORS");
      DECLARE  --(A)
              A : F := 16#0.A179A#E10;
              B : F := 16#0.400CE8#E10;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(A) CHECK +, A, B, A+B ALL MODEL NUMBERS " &
                    "FOR FLOATING POINT");

            IF NOT EQUAL(3,3) THEN      -- NEVER TRUE
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF C /= 16#0.E18688#E10
            OR D /= 16#0.E18688#E10
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(A)

      DECLARE  --(B)
              A : F := 16#0.A0199#E10;
              B : F := 16#0.A0001#E9;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(B) CHECK +, A, B MODEL NUMBERS " &
                 " A+B NOT (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#0.AA199#E10)
            OR (C > 16#0.AA1998#E10)
            OR (D < 16#0.AA199#E10)
            OR (D > 16#0.AA1998#E10)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(B)

      DECLARE  --(C)
              A : F := 16#0.A019A#;
              B : F := 16#0.A019BC#;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(C) CHECK +, A MODEL NUMBER B, A+B NOT "&
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.40335#)
            OR (C > 16#1.40336#)
            OR (D < 16#1.40335#)
            OR (D > 16#1.40336#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(C)

      DECLARE  --(D)

              A : F1 := 16#0.A0018#;
              B : F2 := 16#0.A006#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(D) CHECK +, A, B, A+B ALL MODEL NUMBERS " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF C /= 16#1.40078#
            OR D /= 16#1.40078#
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(D)

      DECLARE  --(E)

              A : F1 := 16#0.A001C#;
              B : F2 := 16#0.A00C#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(E) CHECK +, A, B MODEL NUMBERS, A+B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.400D8#)
            OR (C > 16#1.400E#)
            OR (D < 16#1.400D8#)
            OR (D > 16#1.400E#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(E)

      DECLARE  --(F)
              A : F1 := 16#0.A001C#;
              B : F2 := 16#0.A00CD#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(F) CHECK +, A MODEL NUMBER, B, A+B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.400D8#)
            OR (C > 16#1.401#)
            OR (D < 16#1.400D8#)
            OR (C > 16#1.401#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(F)

      DECLARE  --(G)
              A : F := 16#0.F0269#E10;
              B : F := 16#0.400CE8#E10;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(G) CHECK -, A, B, A-B ALL MODEL NUMBERS " &
                 " FOR FLOATING POINT");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF C /= 16#0.B019A8#E10
            OR D /= 16#0.B019A8#E10
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(G)

      DECLARE  --(H)
              A : F := 16#0.A179B8#;
              B : F := 16#0.30468A#;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(H) CHECK -, A, B, MODEL NUMBERS, A-B NOT " &
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#0.71330C#)
            OR (C > 16#0.713331#)
            OR (D < 16#0.71330C#)
            OR (D > 16#0.713331#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

     END;  --(H)

      DECLARE  --(I)
              A : F := 16#0.A179B8#;
              B : F := 16#0.408ACE#;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(I) CHECK -, A MODEL NUMBER, B, A-B NOT " &
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#0.60EEE8#)
            OR (C > 16#0.60EEEC#)
            OR (D < 16#0.60EEE8#)
            OR (D > 16#0.60EEEC#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(I)

      DECLARE  --(J)
              A : F1 := 16#0.C003C#;
              B : F2 := 16#0.20008#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(J) CHECK -, A, B, A-B ALL MODEL NUMBERS " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF C /= 16#0.A0034#
            OR D /= 16#0.A0034#
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(J)

      DECLARE  --(K)
              A : F1 := 16#A.B1FC#;
              B : F2 := 16#0.700B#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(K) CHECK -, A, B MODEL NUMBERS, A-B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#A.41F#)
            OR (C > 16#A.41F4#)
            OR (D < 16#A.41F#)
            OR (C > 16#A.41F4#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(K)

      DECLARE  --(L)
              A : F1 := 16#0.A001C#;
              B : F2 := -16#0.A00CD#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(L) CHECK -, A MODEL NUMBER, B, A-B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#1.400D8#)
            OR (C > 16#1.401#)
            OR (D < 16#1.400D8#)
            OR (C > 16#1.401#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(L)

      RESULT;

END C45321B;
