-- CA5003B0.ADA

-- WKB 7/22/81
-- JBG 10/6/83
-- BHS 8/02/84
-- JRK 9/20/84


PACKAGE CA5003B0 IS

     ORDER : STRING (1..4) := "    ";

     INDEX : NATURAL := 1;

     FUNCTION SHOW_ELAB (UNIT : CHARACTER) RETURN INTEGER;

END CA5003B0;


PACKAGE BODY CA5003B0 IS

     FUNCTION SHOW_ELAB (UNIT : CHARACTER) RETURN INTEGER IS
     BEGIN
          ORDER (INDEX) := UNIT;
          INDEX := INDEX + 1;
          RETURN INDEX - 1;
     END SHOW_ELAB;

END CA5003B0;
