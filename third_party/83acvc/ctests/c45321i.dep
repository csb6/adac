-- C45321I.DEP

-- OBJECTIVE:
--     TESTS ON ADDING OPERATORS AS 12 SUBTESTS.
--       (A) THE OPERATOR +, A, B, AND A+B ALL MODEL NUMBERS.
--       (B) THE OPERATOR +, A, B MODEL NUMBERS A+B NOT.
--       (C) THE OPERATOR +, A MODEL NUMBER, B, A+B NOT.
--       (D) THE OPERATOR +, A, B, AND A+B ALL MODEL NUMBERS OF
--           DIFFERENT SUBTYPES.
--       (E) THE OPERATOR +, A, B MODEL NUMBERS A+B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (F) THE OPERATOR +, A MODEL NUMBER, B, A+B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (G) THE OPERATOR -, A, B, AND A-B ALL MODEL NUMBERS.
--       (H) THE OPERATOR -, A, B MODEL NUMBERS A-B NOT.
--       (I) THE OPERATOR -, A MODEL NUMBER, B, A-B NOT.
--       (J) THE OPERATOR -, A, B, AND A-B ALL MODEL NUMBERS OF
--           DIFFERENT SUBTYPES.
--       (K) THE OPERATOR -, A, B MODEL NUMBERS A-B NOT, WITH
--           DIFFERENT SUBTYPES.
--       (L) THE OPERATOR -, A MODEL NUMBER, B, A-B NOT, WITH
--           DIFFERENT SUBTYPES.
--      THIS TEST IS TO BE REPEATED FOR 'DIGITS = 5 .. 29.

-- APPLICABILITY CRITERIA:
--     THIS TEST IS APPLICABLE ONLY TO IMPLEMENTATIONS WHICH SUPPORT A
--     VALUE OF DIGITS GREATER THAN OR EQUAL TO 13.

--     IF 'DIGITS 13' IS NOT SUPPORTED, THEN THE DECLARATION OF 'TYPE F'
--     MUST BE REJECTED.

-- HISTORY:
--     BAW 09/17/80
--     JBG 11/10/82
--     JBG 07/29/84
--     JBG 10/12/86 ADD CODE TO ENSURE RUN-TIME ADDING OPERATORS ARE
--              USED BY OPTIMIZING COMPILERS; REMOVE -B FROM TEST NAME.
--     DHH 01/11/88 ADDED APPLICABILITY CRITERIA AND STANDARD HEADER.

WITH REPORT;
PROCEDURE C45321I IS

     USE REPORT;

     N : CONSTANT INTEGER := 13;      -- NUMBER OF DIGITS
     TYPE F IS DIGITS N;             -- MANTISSA BITS: 45  N/A => ERROR.
     SUBTYPE F1 IS F DIGITS N-1;     -- MANTISSA BITS: 41
     SUBTYPE F2 IS F DIGITS N-2;     -- MANTISSA BITS: 38
BEGIN TEST ("C45321I", "CHECK FLOATING POINT ADDING OPERATORS");
      DECLARE  --(A)
              A : F := 16#0.A000000179A#E10;
              B : F := 16#0.400000000CE8#E10;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(A) CHECK +, A, B, A+B ALL MODEL NUMBERS " &
                    "FOR FLOATING POINT");

            IF NOT EQUAL(3,3) THEN      -- NEVER TRUE
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF C /= 16#0.E00000018688#E10
            OR D /= 16#0.E00000018688#E10
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(A)

      DECLARE  --(B)
              A : F := 16#0.A0000000199#E10;
              B : F := 16#0.A0000000001#E9;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(B) CHECK +, A, B MODEL NUMBERS " &
                 " A+B NOT (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#0.AA000000199#E10)
            OR (C > 16#0.AA0000001998#E10)
            OR (D < 16#0.AA000000199#E10)
            OR (D > 16#0.AA0000001998#E10)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(B)

      DECLARE  --(C)
              A : F := 16#0.A000000019A#;
              B : F := 16#0.A000000019BC#;
              C : F := 0.0;
              D : F := A + B;

      BEGIN COMMENT("(C) CHECK +, A MODEL NUMBER B, A+B NOT "&
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.40000000335#)
            OR (C > 16#1.40000000336#)
            OR (D < 16#1.40000000335#)
            OR (D > 16#1.40000000336#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(C)

      DECLARE  --(D)

              A : F1 := 16#0.A000000003#;
              B : F2 := 16#0.A00000000C#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(D) CHECK +, A, B, A+B ALL MODEL NUMBERS " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF C /= 16#1.400000000F#
            OR D /= 16#1.400000000F#
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(D)

      DECLARE  --(E)

              A : F1 := 16#0.A0000000038#;
              B : F2 := 16#0.A000000018#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(E) CHECK +, A, B MODEL NUMBERS, A+B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.4000000006C#)
            OR (C > 16#1.400000001C#)
            OR (D < 16#1.4000000006C#)
            OR (D > 16#1.400000001C#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(E)

      DECLARE  --(F)
              A : F1 := 16#0.A0000000038#;
              B : F2 := 16#0.A000000019A#;
              C : F1 := 0.0;
              D : F1 := A + B;

      BEGIN COMMENT("(F) CHECK +, A MODEL NUMBER, B, A+B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A + B;
            END IF;

            IF (C < 16#1.400000001B#)
            OR (C > 16#1.400000002#)
            OR (D < 16#1.400000001B#)
            OR (C > 16#1.400000002#)
            THEN
                 FAILED("THE RESULT OF THE ADDITION IS INCORRECT");
            END IF;

      END;  --(F)

      DECLARE  --(G)
              A : F := 16#0.F0000000269#E10;
              B : F := 16#0.400000000CE8#E10;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(G) CHECK -, A, B, A-B ALL MODEL NUMBERS " &
                 " FOR FLOATING POINT");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF C /= 16#0.B000000019A8#E10
            OR D /= 16#0.B000000019A8#E10
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(G)

      DECLARE  --(H)
              A : F := 16#0.A000000179B8#;
              B : F := 16#0.30000000468A#;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(H) CHECK -, A, B, MODEL NUMBERS, A-B NOT " &
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#0.70000001330C#)
            OR (C > 16#0.700000013331#)
            OR (D < 16#0.70000001330C#)
            OR (D > 16#0.700000013331#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

     END;  --(H)

      DECLARE  --(I)
              A : F := 16#0.A000000179B8#;
              B : F := 16#0.400000008ACE#;
              C : F := 0.0;
              D : F := A - B;

      BEGIN COMMENT("(I) CHECK -, A MODEL NUMBER, B, A-B NOT " &
                 " (FOR FLOATING POINT)");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#0.60000000EEE8#)
            OR (C > 16#0.60000000EEEC#)
            OR (D < 16#0.60000000EEE8#)
            OR (D > 16#0.60000000EEEC#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(I)

      DECLARE  --(J)
              A : F1 := 16#0.C0000000078#;
              B : F2 := 16#0.2000000001#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(J) CHECK -, A, B, A-B ALL MODEL NUMBERS " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF C /= 16#0.A0000000068#
            OR D /= 16#0.A0000000068#
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(J)

      DECLARE  --(K)
              A : F1 := 16#A.B0000003F8#;
              B : F2 := 16#0.7000000016#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(K) CHECK -, A, B MODEL NUMBERS, A-B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#A.40000003E#)
            OR (C > 16#A.40000003E8#)
            OR (D < 16#A.40000003E#)
            OR (C > 16#A.40000003E8#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(K)

      DECLARE  --(L)
              A : F1 := 16#0.A0000000038#;
              B : F2 := -16#0.A000000019A#;
              C : F1 := 0.0;
              D : F1 := A - B;

      BEGIN COMMENT("(L) CHECK -, A MODEL NUMBER, B, A-B NOT " &
                 " (FOR FLOATING POINT) WITH DIFFERENT SUBTYPES");

            IF NOT EQUAL(3,3) THEN      -- NEVER EXECUTED.
                 A := 0.0;
            END IF;

            IF EQUAL(2,2) THEN
                 C := A - B;
            END IF;

            IF (C < 16#1.400000001B#)
            OR (C > 16#1.400000002#)
            OR (D < 16#1.400000001B#)
            OR (C > 16#1.400000002#)
            THEN
                 FAILED("THE RESULT OF THE SUBTRACTION IS INCORRECT");
            END IF;

      END;  --(L)

      RESULT;

END C45321I;
