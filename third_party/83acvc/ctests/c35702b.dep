-- C35702B.DEP

-- OBJECTIVE:
--     CHECK THAT LONG_FLOAT'DIGITS > FLOAT'DIGITS.

-- APPLICABILITY CRITERIA:
--     THIS TEST IS APPLICABLE TO IMPLEMENTATIONS WHICH SUPPORT
--     THE TYPE "LONG_FLOAT".
--
--     IF THE TYPE "LONG_FLOAT" IS NOT SUPPORTED, THEN THE
--     DECLARATION OF CHECK MUST BE REJECTED.

-- HISTORY:
--     BAW 09/05/80  CREATED ORIGINAL TEST.
--     SPS 02/18/83
--     BCB 01/04/87  MODIFIED HEADER.

WITH REPORT;
PROCEDURE C35702B IS

     USE REPORT;

     CHECK : LONG_FLOAT;                               -- N/A => ERROR.

BEGIN
     TEST ("C35702B","CHECK IF LONG_FLOAT HAS MORE DIGITS THAN FLOAT");

     IF FLOAT'DIGITS >= LONG_FLOAT'DIGITS THEN
          FAILED ("LONG_FLOAT HAS NOT MORE PRECISION THAN FLOAT");
     END IF;

     RESULT;

END C35702B;
