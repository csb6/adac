-- CA1009A4M.ADA

-- CHECK THAT A NON-GENERIC SUBPROGRAM DECLARATION CAN BE
-- COMPILED SEPARATELY.

-- SEPARATE FILES ARE:
--   CA1009A0  A LIBRARY PROCEDURE DECLARATION.
--   CA1009A1  A LIBRARY PROCEDURE BODY (CA1009A0).
--   CA1009A2  A LIBRARY FUNCTION DECLARATION.
--   CA1009A3  A LIBRARY FUNCTION BODY (CA1009A2).
--   CA1009A4M THE MAIN PROCEDURE.

-- JRK 5/20/81

WITH REPORT, CA1009A0, CA1009A2;
USE REPORT;

PROCEDURE CA1009A4M IS

     I : INTEGER := 0;

BEGIN
     TEST ("CA1009A", "NON-GENERIC SUBPROGRAM DECLARATIONS " &
           "COMPILED SEPARATELY");

     CA1009A0 (I);
     IF I /= 1 THEN
          FAILED ("LIBRARY PROCEDURE NOT ELABORATED/EXECUTED");
     END IF;

     IF CA1009A2(5) /= -5 THEN
          FAILED ("LIBRARY FUNCTION NOT ELABORATED/EXECUTED");
     END IF;

     RESULT;
END CA1009A4M;
