-- CA1020D0.ADA

-- GENERIC UNITS TO BE INSTANTIATED AS LIBRARY UNITS.

-- JBG 5/28/85

GENERIC
     C : INTEGER;
PROCEDURE GENPROC_CA1020D (X : OUT INTEGER);

WITH REPORT; USE REPORT;
PRAGMA ELABORATE (REPORT);
PROCEDURE GENPROC_CA1020D (X : OUT INTEGER) IS
BEGIN
     X := IDENT_INT(C);
END GENPROC_CA1020D;

GENERIC
FUNCTION GENFUNC_CA1020D RETURN INTEGER;

FUNCTION GENFUNC_CA1020D RETURN INTEGER IS
BEGIN
     RETURN 2;
END GENFUNC_CA1020D;

WITH GENPROC_CA1020D;
PRAGMA ELABORATE (GENPROC_CA1020D);
PROCEDURE CA1020D_PROC1 IS NEW GENPROC_CA1020D(1);

WITH GENFUNC_CA1020D;
PRAGMA ELABORATE (GENFUNC_CA1020D);
FUNCTION CA1020D_FUNC2 IS NEW GENFUNC_CA1020D; 
