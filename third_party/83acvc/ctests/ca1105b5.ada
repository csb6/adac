-- CA1105B5.ADA

-- WKB 6/23/81
-- BHS 7/25/84

WITH CA1105B0, CA1105B1, CA1105B2;
WITH CA1105B3M;
WITH REPORT; USE REPORT;
SEPARATE (CA1105B3M.CA1105B4)
PROCEDURE CA1105B5 IS

     K : INTEGER := 1;

BEGIN

     K := CA1105B0.I;
     IF K /= 0 THEN
          FAILED ("LIBRARY PACKAGE NOT ACCESSED - 3");
     END IF;

     K := 0;
     CA1105B1(K);
     IF K /= 1 THEN
          FAILED ("LIBRARY PROCEDURE NOT INVOKED - 4");
     END IF;

     K := 1;
     IF CA1105B2(K) /= -1 THEN
          FAILED ("LIBRARY FUNCTION NOT INVOKED - 5");
     END IF;

END CA1105B5;
