-- CA1105A1M.ADA

-- CHECK THAT A WITH_CLAUSE CAN NAME A LIBRARY UNIT SPECIFIED
--   IN THE SAME OR A PREVIOUS WITH_CLAUSE.

-- SEPARATE FILES ARE:
--   CA1105A0  A LIBRARY PACKAGE.
--   CA1105A1M THE MAIN PROCEDURE.

-- WKB 6/22/81

WITH REPORT, CA1105A0;
USE REPORT;
WITH CA1105A0;
WITH CA1105A0, REPORT, CA1105A0;

PROCEDURE CA1105A1M IS

BEGIN
     TEST ("CA1105A", "SAME UNIT NAME IN TWO WITH_CLAUSES OF A " &
                      "CONTEXT_SPECIFICATION");

     IF CA1105A0.I /= 1 THEN
          FAILED ("LIBRARY PACKAGE NOT VISIBLE");
     END IF;

     RESULT;
END CA1105A1M;
