-- AVAT409.ADA

-- LIST VALUES OF INTEGER ATTRIBUTES.

-- PWB 08/18/87  CREATED ORIGINAL TEST.

WITH DATA_COLLECTION; USE DATA_COLLECTION;
WITH SYSTEM; USE SYSTEM;

PROCEDURE AVAT409 IS
     TESTED : THINGS_TO_SUPPORT := INTEGER_VALUES;

     LINE1 : CONSTANT STRING := "     ATTRIBUTE VALUES FOR TYPE " &
                                "INTEGER:";
     LINE2 : CONSTANT STRING := "          INTEGER'FIRST = " &
                                INTEGER'IMAGE (INTEGER'FIRST);
     LINE3 : CONSTANT STRING := "          INTEGER'LAST =" &
                                INTEGER'IMAGE (INTEGER'LAST);
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT409;
