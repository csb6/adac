-- AVAT006.ADA

-- IF THERE IS A PRE-DEFINED INTEGER TYPE OTHER THAN INTEGER,
-- SHORT_INTEGER, OR LONG_INTEGER, THEN THIS UNIT WILL BE REPLACED.

-- CHECK WHETHER OTHER PREDEFINED INTEGER TYPES ARE SUPPORTED.

-- PWB 01/15/87  CREATED ORIGINAL TEST.
-- TBN 03/20/87  CHANGED FORMAT.

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT006 IS
     X : INTEGER;
     TESTED : THINGS_TO_SUPPORT := OTHER_INT;
     LINE1 : CONSTANT STRING := "     OTHER PREDEFINED INTEGER TYPES " &
                                "ARE NOT SUPPORTED.";
     LINE2 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TESTS ARE INAPPLICABLE:";
     LINE3 : CONSTANT STRING := "               B86001X  C35404D  " &
                                "C45231D  C86006E  CD7101G";
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT006;
