-- AVAT014.ADA

-- CHECK WHETHER THERE ARE OTHER PREDEFINED FIXED POINT TYPES SUPPORTED
-- OTHER THAN DURATION.  IF SO, THIS UNIT WILL BE REPLACED.

-- TBN 11/18/87  ADAPTED FROM AVAT012.

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT014 IS
     X : DURATION;
     TESTED : THINGS_TO_SUPPORT := OTHER_FIX;
     LINE1 : CONSTANT STRING := "     OTHER PREDEFINED FIXED " &
                                "POINT TYPES ARE NOT SUPPORTED.";
     LINE2 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TEST IS INAPPLICABLE:";
     LINE3 : CONSTANT STRING := "               B86001Y";
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT014;
