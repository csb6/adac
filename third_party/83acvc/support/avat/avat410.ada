-- AVAT410.ADA

-- LIST VALUES OF ATTRIBUTES OF TYPE DURATION.

-- PWB 08/18/87  CREATED ORIGINAL TEST.

WITH DATA_COLLECTION; USE DATA_COLLECTION;
WITH SYSTEM; USE SYSTEM;
WITH TEXT_IO;

PROCEDURE AVAT410 IS

     TESTED : THINGS_TO_SUPPORT := DURATION_VALUES;

     TYPE FLT IS DIGITS MAX_DIGITS;
     PACKAGE FLT_IO IS NEW TEXT_IO.FLOAT_IO (FLT);

     DELTA_LABEL : CONSTANT STRING := "          DURATION'DELTA =";
     DELTA_LABEL_LEN : NATURAL := DELTA_LABEL'LENGTH;

     FIRST_LABEL : CONSTANT STRING := "          DURATION'FIRST =";
     FIRST_LABEL_LEN : NATURAL := FIRST_LABEL'LENGTH;

     LAST_LABEL : CONSTANT STRING := "          DURATION'LAST =";
     LAST_LABEL_LEN : NATURAL := LAST_LABEL'LENGTH;

     LINE1 : CONSTANT STRING := "     ATTRIBUTE VALUES FOR TYPE " &
                                "DURATION:";

     LINE2 : STRING (1 .. FIRST_LABEL_LEN + 9 + FLT'DIGITS);

     LINE3 : STRING (1 .. LAST_LABEL_LEN + 9 + FLT'DIGITS);

     LINE4 : STRING (1 .. DELTA_LABEL_LEN + 9 + FLT'DIGITS);

BEGIN
     LINE2 (1 .. FIRST_LABEL_LEN) := FIRST_LABEL;
     FLT_IO.PUT ( LINE2 (1 + FIRST_LABEL_LEN .. LINE2'LENGTH),
                       FLT(DURATION'FIRST), EXP => 4 );

     LINE3 (1 .. LAST_LABEL_LEN) := LAST_LABEL;
     FLT_IO.PUT ( LINE3 (1 + LAST_LABEL_LEN .. LINE3'LENGTH),
                       FLT(DURATION'LAST), EXP => 4 );

     LINE4 (1 .. DELTA_LABEL_LEN) := DELTA_LABEL;
     FLT_IO.PUT ( LINE4 (1 + DELTA_LABEL_LEN .. LINE4'LENGTH),
                       FLT(DURATION'DELTA), EXP => 4 );

     FEATURE (TESTED) := (4, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3),
                              (LINE4'LENGTH, LINE4)));
END AVAT410;
