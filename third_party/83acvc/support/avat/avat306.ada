-- AVAT306.ADA

-- CHECK WHETHER COMPILATION OF GENERIC LIBRARY PACKAGE BODY APART
-- FROM SPEC IS SUPPORTED.
-- IF SO, THE BODY OF THE GENERIC PACKAGE AVAT306 WILL BE REPLACED.

-- FILES FOR THIS SUBTEST:
--     AVAT306.ADA: SPEC AND BODY OF GENERIC PACK AVAT306 (THIS FILE).
--     AVAT307.ADA: REPLACEMENT BODY OF GENERIC PACK AVAT306.
--     AVAT308.ADA: MAIN PROCEDURE, WITH INSTANTIATION OF AVAT306.

-- PWB 01/15/87
-- TBN 03/25/87
-- PWB 08/19/87  RENUMBERED AND CHANGED FORMAT.
-- DHH 08/01/89  UPDATED LIST FOR ACVC 1.11

WITH DATA_COLLECTION; USE DATA_COLLECTION;

GENERIC
PACKAGE AVAT306 IS
     PROCEDURE TESTIT;
END AVAT306;

PACKAGE BODY AVAT306 IS
     PROCEDURE TESTIT IS
          TESTED : THINGS_TO_SUPPORT := SEP_LIB_PACK_BODY;
          LINE1 : CONSTANT STRING := "     SEPARATE COMPILATION OF " &
                                     "GENERIC LIBRARY PACKAGE " &
                                     "SPECIFICATION AND";
          LINE2 : CONSTANT STRING := "     BODY IS NOT SUPPORTED.";
          LINE3 : CONSTANT STRING := "          THIS INDICATES THAT " &
                                     "THE FOLLOWING TESTS ARE " &
                                     "INAPPLICABLE:";
          LINE4 : CONSTANT STRING := "               B83004B  " &
                                     "B83004D  B83004F  BC3204C  " &
                                     "BC3204D";
     BEGIN
          FEATURE (TESTED) := (4, ((LINE1'LENGTH, LINE1),
                                   (LINE2'LENGTH, LINE2),
                                   (LINE3'LENGTH, LINE3),
                                   (LINE4'LENGTH, LINE4)));
     END TESTIT;
END AVAT306;
