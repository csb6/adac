-- AVATMAIN.ADA

-- IDENTIFY IMPLEMENTATION CHARACTERISTICS WHICH CAUSE CERTAIN TESTS TO
-- BE NON-APPLICABLE FOR PREDEFINED NUMERIC TYPES AND PRECISION-RELATED
-- FEATURES.

-- TBN 11/17/87  CREATED ORIGINAL FILE.
-- DHH 08/01/89  MODIFIED FOR ACVC 1.11, COMBINED SEPARATE FILES INTO
--               ONE, AND REDUCED FILES TO REFLECT FEWER .DEP TESTS.

WITH DATA_COLLECTION; USE DATA_COLLECTION;
WITH AVAT000, AVAT001, AVAT002, AVAT004, AVAT006, AVAT008, AVAT010,
     AVAT012, AVAT014, AVAT016, AVAT017, AVAT020, AVAT023, AVAT030,
     AVAT031, AVAT101, AVAT134, AVAT144, AVAT145, AVAT201, AVAT208,
     AVAT220, AVAT301, AVAT303, AVAT308, AVAT312, AVAT316, AVAT319,
     AVAT322, AVAT323, AVAT324, AVAT325, AVAT326, AVAT327, AVAT329,
     AVAT331, AVAT333, AVAT401, AVAT402, AVAT403, AVAT404, AVAT405,
     AVAT406, AVAT407, AVAT408, AVAT409, AVAT410, AVAT411;

PROCEDURE AVATMAIN IS

BEGIN

     -- PREDEFINED NUMERIC TYPES.

     AVAT000;     -- HEADER FOR PART ONE.
     AVAT001;     -- HEADER COMMENT.
     AVAT002;     -- SHORT_INTEGER.
     AVAT004;     -- LONG_INTEGER.
     AVAT006;     -- OTHER PREDEFINED INTEGER.
     AVAT008;     -- SHORT_FLOAT.
     AVAT010;     -- LONG_FLOAT.
     AVAT012;     -- OTHER PREDEFINED FLOATING POINT.
     AVAT014;     -- OTHER PREDEFINED FIXED POINT.

     -- CHECKS RELATED TO PRECISION.

     AVAT016;     -- HEADER COMMENT.
     AVAT017;     -- CHECK MAX_MANTISSA.
     AVAT030;     -- LIST OF MAX_MANTISSA N/A TESTS.
     AVAT020;     -- CHECK MAX_DIGITS.
     AVAT023;     -- CHECK FINE_DELTA.
     AVAT031;     -- LIST OF FINE_DELTA N/A TESTS.

     -- COMPILATION OF SIZE TESTS

     AVAT101;     -- HEADER FOR ACCESS 'SIZE CLAUSES.
     AVAT134;     -- LIBRARY UNIT FOR NON-DEFAULT ACCESS 'SIZE CLAUSES.
     AVAT144;     -- HEADER FOR NON-BINARY, NON-DECIMAL 'SMALL CLAUSE
                  -- LISTS.
     AVAT145;     -- LIBRARY UNIT FOR NON-BINARY, NON-DECIMAL 'SMALL
                  -- CLAUSES.

     -- ENUMERATION REPRESENTATION CLAUSES

     AVAT201;     -- REPRESENTATION CLAUSE HEADER.
     AVAT208;     -- NONDEFAULT REP CLAUSE FOR A BOOLEAN TYPE.

     -- MACHINE CODE CLAUSES

     AVAT220;     -- MACHINE_CODE CHECK.

     -- COMPILATION OF GENERICS

     AVAT301;     -- HEADER COMMENT FOR COMPILATION OF GENERICS.

     AVAT303;     -- GENERIC LIBRARY SUBPR BODY APART FROM SPEC.
     AVAT308;     -- GENERIC LIBRARY PACKAGE BODY APART FROM SPEC.
     AVAT312;     -- GENERIC NONLIB SUBPR BODY SUBUNIT APART FROM STUB.
     AVAT316;     -- GENERIC NONLIB PACK BODY SUBUNIT APART FROM STUB.
     AVAT319;     -- SUBPR BODY AS SUBUNIT OF GEN SUBPR APART FROM STUB.
     AVAT322;     -- PACK BODY AS SUBUNIT OF GEN SUBPR APART FROM STUB.

     -- INPUT/OUTPUT SUPPORT

     AVAT323;     -- HEADER COMMENT FOR INPUT/OUTPUT SUPPORT.

     AVAT324;     -- CREATION OF TEXT FILES.
     AVAT325;     -- CREATION OF SEQUENTIAL FILES.
     AVAT326;     -- CREATION OF DIRECT ACCESS FILES.
     AVAT327;     -- INSTANTIATION OF SEQ_IO WITH UNCONSTR ARRAY TYPE.
     AVAT329;     -- INSTANTIATION OF SEQ_IO WITH NO-DEFAULT DISCR REC.
     AVAT331;     -- INSTANTIATION OF DIR_IO WITH UNCONSTR ARRAY TYPE.
     AVAT333;     -- INSTANTIATION OF DIR_IO WITH NO-DEFAULT DISCR REC.

     AVAT401;     -- HEADER COMMENT FOR VALUES FROM PACKAGE SYSTEM.

     AVAT402;     -- VALUES OF TYPE NAME.
     AVAT403;     -- VALUES OF STORAGE_UNIT AND MEMORY_SIZE.
     AVAT404;     -- VALUES OF MIN_INT AND MAX_INT.
     AVAT405;     -- VALUES OF MAX_DIGITS, MAX_MANTISSA, AND FINE_DELTA.
     AVAT406;     -- VALUES OF PRIORITY'FIRST, PRIORITY'LAST, AND TICK.

     -- ATTRIBUTES FROM PACKAGE STANDARD

     AVAT407;     -- HEADER COMMENT FOR VALUES FROM PACKAGE STANDARD.
     AVAT408;     -- VALUES OF FLOAT'FIRST, FLOAT'LAST, FLOAT'DIGITS.
     AVAT409;     -- VALUES OF INTEGER'FIRST AND INTEGER'LAST.
     AVAT410;     -- VALUES OF DURATION'FIRST, 'LAST, AND 'DELTA.
     AVAT411;     -- TRAILER FOR AVAT;

     -- END OF THINGS TO SUPPORT

     REPORT_DATA (HEADER_PART_ONE, TRAILER_PART_FIVE);

END AVATMAIN;
