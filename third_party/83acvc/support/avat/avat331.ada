-- AVAT331.ADA

-- CHECK WHETHER THE INSTANTIATION OF DIRECT_IO WITH UNCONSTRAINED
-- ARRAY TYPES IS SUPPORTED.
-- IF SO, THIS UNIT WILL BE REPLACED.

-- TBN 03/26/87
-- PWB 08/20/87  RENUMBERED.

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT331 IS
     TESTED : THINGS_TO_SUPPORT := DIR_UNCONSTR_ARRAY;
     LINE1 : CONSTANT STRING := "     INSTANTIATION OF DIRECT_IO " &
                                "WITH UNCONSTRAINED ARRAY TYPES IS";
     LINE2 : CONSTANT STRING := "     NOT SUPPORTED.";
     LINE3 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TESTS ARE INAPPLICABLE:";
     LINE4 : CONSTANT STRING := "               AE2101H  EE2401D";
BEGIN
     FEATURE (TESTED) := (4, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3),
                              (LINE4'LENGTH, LINE4)));
END AVAT331;
