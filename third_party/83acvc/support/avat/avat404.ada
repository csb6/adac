-- AVAT404.ADA

-- LIST VALUES OF SYSTEM.MIN_INT AND SYSTEM.MAX_INT.

-- PWB 08/18/87  CREATED ORIGINAL TEST.

WITH DATA_COLLECTION; USE DATA_COLLECTION;
WITH SYSTEM; USE SYSTEM;

PROCEDURE AVAT404 IS

     TESTED : THINGS_TO_SUPPORT := INTEGER_LIMITS;
     TYPE LARGE_INT IS RANGE SYSTEM.MIN_INT .. SYSTEM.MAX_INT;

     LINE1 : CONSTANT STRING := "     INTEGER TYPE LIMITS:";
     LINE2 : CONSTANT STRING := "          SYSTEM.MIN_INT =" &
                                LARGE_INT'IMAGE(MIN_INT);
     LINE3 : CONSTANT STRING := "          SYSTEM.MAX_INT =" &
                                LARGE_INT'IMAGE(MAX_INT);
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT404;
