-- AVAT010.ADA

-- CHECK WHETHER LONG_FLOAT IS SUPPORTED.
-- IF SO, THIS UNIT WILL BE REPLACED.

-- PWB 01/15/87  CREATED ORIGINAL TEST.
-- TBN 03/25/87  CHANGED FORMAT.

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT010 IS
     X : FLOAT;
     TESTED : THINGS_TO_SUPPORT := LONG_FLO;
     LINE1 : CONSTANT STRING := "     LONG_FLOAT IS NOT SUPPORTED.";
     LINE2 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TESTS ARE INAPPLICABLE:";
     LINE3 : CONSTANT STRING := "               B86001U  C35702B  " &
                                "C35713C  C86006G";
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT010;
