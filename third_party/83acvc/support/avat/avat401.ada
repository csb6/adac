WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT401 IS
     TESTED : THINGS_TO_SUPPORT := SYSTEM_REPORT;
     LINE1 : CONSTANT STRING := "====== VALUES DEFINED IN PACKAGE " &
                                "SYSTEM ======";
BEGIN
     FEATURE (TESTED) := (1, (1 => (LINE1'LENGTH, LINE1)));
END AVAT401;
