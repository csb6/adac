-- AVAT302.ADA

-- CHECK WHETHER COMPILATION OF GENERIC LIBRARY SUBPROGRAM BODY APART
-- FROM SPEC IS SUPPORTED.
-- IF SO, THIS UNIT WILL BE REPLACED.

-- FILES FOR THIS SUBTEST:
--     AVAT302.ADA: SPEC AND BODY OF GENERIC PROC AVAT302 (THIS FILE).
--     AVAT303.ADA: REPLACEMENT SPEC OF GENERIC PROC AVAT302.
--     AVAT304.ADA: REPLACEMENT BODY OF GENERIC PROC AVAT302.
--     AVAT305.ADA: MAIN PROCEDURE, WITH INSTANTIATION OF AVAT302.

-- PWB 01/15/87
-- TBN 03/25/87
-- PWB 08/19/87  RENUMBERED AND CHANGED FORMAT.
-- DHH 08/01/89  UPDATED LIST FOR ACVC 1.11

WITH DATA_COLLECTION; USE DATA_COLLECTION;

GENERIC
PROCEDURE AVAT302;

PROCEDURE AVAT302 IS
     TESTED : THINGS_TO_SUPPORT := SEP_LIB_SUBPR_BODY;
     LINE1 : CONSTANT STRING := "     SEPARATE COMPILATION OF " &
                                "GENERIC LIBRARY SUBPROGRAM";
     LINE2 : CONSTANT STRING := "     SPECIFICATION AND BODY IS NOT" &
                                " SUPPORTED.";
     LINE3 : CONSTANT STRING := "          THIS INDICATES THAT THE" &
                                " FOLLOWING TEST IS INAPPLICABLE:";
     LINE4 : CONSTANT STRING := "               BA1010D  CA1012A";
BEGIN
     FEATURE (TESTED) := (4, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3),
                              (LINE4'LENGTH, LINE4)));
END AVAT302;
