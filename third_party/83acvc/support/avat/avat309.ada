-- AVAT309.ADA

-- CHECK WHETHER COMPILATION OF GENERIC NONLIBRARY SUBPROGRAM BODY
-- AS SUBUNIT APART FROM SPEC IS SUPPORTED.
-- IF SO, THE BODY OF PACKAGE AVAT309_PACK WILL BE REPLACED.

-- FILES FOR THIS SUBTEST:
--     AVAT309.ADA: PACKAGE SPEC AND BODY (AVAT309_PACK) WITH BODY
--                   STUB AND PROPER BODY FOR GENERIC FUNCTION
--                   AVAT309 (THIS FILE).
--     AVAT310.ADA: REPLACEMENT BODY FOR AVAT309_PACK
--     AVAT311.ADA: REPLACEMENT SUBUNIT PROPER BODY FOR GENERIC
--                   FUNCTION AVAT309.
--     AVAT312.ADA: MAIN PROCEDURE WITH INSTANTIATION OF AVAT309.

-- TBN 03/25/87
-- PWB 08/19/87  RENUMBERED.
-- RJW 03/28/88  CORRECTED OUTPUT MESSAGE.

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PACKAGE AVAT309_PACK IS
     TESTED : THINGS_TO_SUPPORT := NONLIB_SUBPR_SUBUNIT;
     LINE1 : CONSTANT STRING := "     COMPILATION OF GENERIC " &
                                "NONLIBRARY SUBPROGRAM BODY AS";
     LINE2 : CONSTANT STRING := "     SUBUNIT APART FROM SPEC IS " &
                                "NOT SUPPORTED.";
     LINE3 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TEST IS INAPPLICABLE:";
     LINE4 : CONSTANT STRING := "               CA2009C";
     GENERIC
     FUNCTION AVAT309 RETURN BOOLEAN;
END AVAT309_PACK;

PACKAGE BODY AVAT309_PACK IS
     FUNCTION AVAT309 RETURN BOOLEAN IS SEPARATE;
END AVAT309_PACK;

SEPARATE (AVAT309_PACK)
FUNCTION AVAT309 RETURN BOOLEAN IS
BEGIN
     RETURN TRUE;
END AVAT309;
