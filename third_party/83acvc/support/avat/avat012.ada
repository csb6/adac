-- AVAT012.ADA

-- CHECK WHETHER PREDEFINED FLOATING POINT TYPES OTHER THAN FLOAT,
-- SHORT_FLOAT, OR LONG_FLOAT ARE SUPPORTED.  IF SO, THIS UNIT WILL
-- BE REPLACED.

-- PWB 08/11/87  ADAPTED FROM AVAT006

WITH DATA_COLLECTION; USE DATA_COLLECTION;

PROCEDURE AVAT012 IS
     X : FLOAT;
     TESTED : THINGS_TO_SUPPORT := OTHER_FLO;
     LINE1 : CONSTANT STRING := "     OTHER PREDEFINED FLOATING " &
                                "POINT TYPES ARE NOT SUPPORTED.";
     LINE2 : CONSTANT STRING := "          THIS INDICATES THAT THE " &
                                "FOLLOWING TESTS ARE INAPPLICABLE:";
     LINE3 : CONSTANT STRING := "               B86001Z  C35713D";
BEGIN
     FEATURE (TESTED) := (3, ((LINE1'LENGTH, LINE1),
                              (LINE2'LENGTH, LINE2),
                              (LINE3'LENGTH, LINE3)));
END AVAT012;
