-- BB2003C.ADA

-- CHECK THAT DUPLICATE OTHERS CHOICE IN AN EXCEPTION-
-- HANDLER IS FORBIDDEN.

-- DCB 05/07/80
-- JRK 11/17/80
-- ABW 06/10/82

PROCEDURE BB2003C IS

     I1 : INTEGER;

BEGIN
     I1 := 1;
EXCEPTION
     WHEN OTHERS =>
          NULL;
     WHEN OTHERS =>     -- ERROR: MORE THAN ONE OTHERS HANDLER.
          NULL;

END BB2003C;
