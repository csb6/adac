-- B54A05B.ADA

-- CHECK THAT NON-DISCRETE (FIXED) TYPES ARE NOT PERMITTED
--    IN CASE EXPRESSIONS.

-- DAT 1/29/81

PROCEDURE B54A05B IS

     TYPE FIXED_INT IS DELTA 1.0 RANGE 0.0 .. 1.0;
     F1 : CONSTANT FIXED_INT := 1.0;

BEGIN

     CASE F1 IS               -- ERROR: FIXED TYPE.
          WHEN OTHERS => NULL;
     END CASE;

END B54A05B;
