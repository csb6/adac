-- BA1101E.ADA

-- CHECK THAT THE NAME OF THE UNIT BEING COMPILED CANNOT BE IN
--    A WITH_CLAUSE WHEN A LIBRARY UNIT IS BEING DECLARED.

-- WKB 6/19/81
-- BHS 7/19/84

PACKAGE BA1101E0 IS END BA1101E0;

WITH BA1101E0; 
PACKAGE BA1101E0 IS             -- ERROR: SAME NAME.

     X : INTEGER;

END BA1101E0;

PACKAGE BA1101E1 IS END BA1101E1;

WITH BA1101E1;
PROCEDURE BA1101E1 IS           -- ERROR: SAME NAME.

     I : INTEGER;

BEGIN

     I := 1;

END BA1101E1;


PACKAGE BA1101E2 IS END BA1101E2;

WITH BA1101E2;
GENERIC
     TYPE ITEM IS RANGE <>;
     Y : ITEM;
PACKAGE BA1101E2 IS            -- ERROR: SAME NAME.
END BA1101E2;


PACKAGE BA1101E3 IS END BA1101E3;

WITH BA1101E3;
GENERIC
     TYPE ITEM IS RANGE <>;
     Z : ITEM;
FUNCTION BA1101E3 RETURN ITEM;           -- ERROR: SAME NAME.

FUNCTION BA1101E3 RETURN ITEM IS         -- OPTIONAL ERROR MSG.
BEGIN

     RETURN Z;                           -- OPTIONAL ERROR MSG.

END BA1101E3;
