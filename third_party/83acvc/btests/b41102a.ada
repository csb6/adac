-- B41102A.ADA

-- CHECK THAT AN INDEXED NAME MAY NOT BE AN AGGREGATE, AN
--   ALLOCATOR, OR AN IDENTIFIER DENOTING AN ACCESS OBJECT
--   WHOSE VALUE DESIGNATES ANOTHER ACCESS OBJECT WHOSE
--   VALUE DESIGNATES AN ARRAY.

-- WKB 7/27/81
-- SPS 2/9/83

PROCEDURE B41102A IS

     I : INTEGER := 1;
     J : INTEGER;

     TYPE A IS ARRAY (INTEGER RANGE 1..5) OF INTEGER;
     TYPE AA IS ACCESS A;
     TYPE AAA IS ACCESS AA;

     AC : AAA;

BEGIN

     J := (2,4,6,8,10) (I);        -- ERROR: INDEXED AGGREGATE.
     NULL;

END B41102A;
