--BE3002E.ADA

-- CHECK THAT UNBOUNDED IS A CONSTANT OF TYPE COUNT.

-- SPS 10/1/82
-- JBG 3/16/83

WITH TEXT_IO;
USE TEXT_IO;

PROCEDURE BE3002E IS

     X : COUNT := 3;
     Y : COUNT := 20;
     Z : CONSTANT := COUNT'FIRST;       -- ERROR: NOT UNIVERSAL INTEGER.
     B : BOOLEAN := X = COUNT'FIRST;    -- OK.

BEGIN
     UNBOUNDED := X;                    -- ERROR: ASSIGNMENT TO CONST.
     UNBOUNDED := 16;                   -- ERROR: ASSIGNMENT TO CONST.
END BE3002E;
