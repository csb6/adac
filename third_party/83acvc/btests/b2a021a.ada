-- B2A021A.ADA

-- CHECK THAT A STRING LITERAL DELIMITED BY PERCENT SIGNS CANNOT CONTAIN
-- A QUOTATION CHARACTER.

-- JBG 5/25/85

PROCEDURE B2A021A IS
     X : STRING (1..5) := %12"45%;      -- ERROR: " NOT ALLOWED.
     B : BOOLEAN;
BEGIN
     B := %AB% = %""%;                  -- ERROR: " NOT ALLOWED.
END B2A021A;
