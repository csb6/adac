-- B52003A.ADA

-- CHECK THAT MULTIPLE ASSIGNMENTS ARE NOT PERMITTED
--    WITHIN A SINGLE ASSIGNMENT STATEMENT.

-- DCB 2/1/80
-- SPS 03/21/83
-- RJK 9/13/83

PROCEDURE B52003A IS

     I1, I2, I3, I4 : INTEGER;

BEGIN

     I1, I2 := 45;              -- ERROR: MULTIPLE ASSSIGNMENT.
     I1 := 46;

END B52003A;
