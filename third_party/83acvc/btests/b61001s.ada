-- B61001S.ADA

-- CHECK THAT THE RETURN TYPE OF A FUNCTION MUST BE DESIGNATED BY A TYPE
-- MARK; IT CANNOT HAVE A RANGE CONSTRAINT.

-- BHS 6/28/84

PROCEDURE B61001S IS 

     FUNCTION F4 (X : INTEGER) RETURN INTEGER RANGE 1..10 IS  -- ERROR: 
                                                    -- RANGE CONSTRAINT 
     BEGIN
          RETURN 4;
     END F4;

BEGIN

     NULL;

END B61001S;
