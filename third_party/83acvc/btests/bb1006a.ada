-- BB1006A.ADA

-- CHECK THAT SELECT_ERROR IS NO LONGER A PREDEFINED EXCEPTION.

-- TBN 2/5/86

PROCEDURE BB1006A IS

BEGIN
     RAISE SELECT_ERROR;               -- ERROR: UNDEFINED EXCEPTION.
EXCEPTION
     WHEN SELECT_ERROR =>              -- ERROR: UNDEFINED EXCEPTION.
          NULL;
     WHEN OTHERS =>
          NULL;
END BB1006A;
