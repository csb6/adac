-- B55A01I.ADA

-- CHECK ILLEGAL SYNTAX IN END LOOP STATEMENT.

-- CHECK 'END WHILE' STATEMENT.

-- DAT 1/21/81
-- ABW 6/17/82
-- SPS 3/1/83

PROCEDURE B55A01I IS

     VBF : BOOLEAN := FALSE;

BEGIN

     WHILE VBF LOOP
          NULL;
     END WHILE;                    -- ERROR: END LOOP REQUIRED.

END B55A01I;
