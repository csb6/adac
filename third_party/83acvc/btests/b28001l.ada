-- B28001L.ADA

-- CHECK THAT PRAGMA LIST IS NOT ALLOWED AS THE ONLY STATEMENT IN A
-- SEQUENCE OF STATEMENTS IN A PACKAGE BODY.

-- TBN  9/11/86

PROCEDURE B28001L IS

     PACKAGE P IS
          A : INTEGER;
     END P;

     PACKAGE BODY P IS
     BEGIN
          PRAGMA LIST (ON);
     END P;                                 -- ERROR: MISSING STATEMENT.

BEGIN
     NULL;
END B28001L;
