-- B38204A.ADA

-- CHECK THAT THE PREFIX OF TASKING ATTRIBUTES CANNOT BE A
-- TYPE MARK DENOTING AN ACCESS TYPE.

-- AH  8/29/86

PROCEDURE B38204A IS
     TASK TYPE T1;
     TYPE ACC1 IS ACCESS T1;

     T_OBJ : T1;
     B : BOOLEAN;

     TASK BODY T1 IS
     BEGIN
          NULL;
     END T1;

BEGIN
     B := T_OBJ'CALLABLE;         -- OK.
     B := T_OBJ'TERMINATED;       -- OK.

     B := ACC1'CALLABLE;          -- ERROR: ACC1.
     B := ACC1'TERMINATED;        -- ERROR: ACC1.
END B38204A;
