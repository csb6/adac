-- B35A01A.ADA

-- CHECK THAT THE PREFIX OF DELTA, FORE, AND AFT CANNOT BE A FLOATING 
-- POINT TYPE.

-- RJW 2/20/86

PROCEDURE B35A01A IS

     TYPE FLOAT IS DIGITS 3 RANGE -1.0 .. 1.0;
     F : FLOAT;
     I : INTEGER;
BEGIN
     F := FLOAT'DELTA;   -- ERROR: PREFIX OF 'DELTA' IS FLOATING POINT.
     I := FLOAT'FORE;    -- ERROR: PREFIX OF 'FORE' IS FLOATING POINT.
     I := FLOAT'AFT;     -- ERROR: PREFIX OF 'AFT' IS FLOATING POINT.

END B35A01A;
