-- B86001U.DEP

-- OBJECTIVE:
--     CHECK THAT A LIBRARY UNIT CANNOT HAVE THE NAME  'LONG_FLOAT'.

-- APPLICABILITY CRITERIA:
--     THIS TEST IS APPLICABLE ONLY TO THOSE IMPLEMENTATIONS THAT
--     SUPPORT LONG_FLOAT.
--
--     IF LONG_FLOAT IS NOT SUPPORTED, THEN THE DECLARATION OF
--     "CHECK_VAR" MUST BE REJECTED.

-- HISTORY:
--     RM   01/16/81  CREATED ORIGINAL TEST.
--     JRK  02/24/83
--     BCB  11/13/87  CHANGED HEADER TO STANDARD FORMAT.  RENAMED FROM
--                    B86001CQ.DEP.

PROCEDURE B86001U IS

     CHECK_VAR : LONG_FLOAT;                           -- N/A => ERROR.

BEGIN
     NULL;
END B86001U;

PROCEDURE  LONG_FLOAT  IS    -- ERROR: ILLEGAL NAME.

     A : BOOLEAN ;

BEGIN

     NULL ;

END LONG_FLOAT ;
