-- B91002F.ADA

-- CHECK THAT DECLARATIONS OTHER THAN ENTRY DECLARATIONS ARE NOT ALLOWED
-- IN TASK SPECIFICATIONS.

-- (F) RECORD TYPE DECLARATION.

-- JRK 9/16/81
-- JBG 4/19/83

PROCEDURE B91002F IS

     TASK TYPE T2F IS

          ENTRY E;

          TYPE R IS                  -- ERROR: PROHIBITED IN TASK SPEC.
               RECORD
                    B : BOOLEAN;
               END RECORD;

     END T2F;

     TASK BODY T2F IS
     BEGIN
          NULL;
     END;

BEGIN
     NULL;
END B91002F;
