-- B85008E.ADA

-- OBJECTIVE:
--     CHECK THAT AN EXCEPTION RENAMING DECLARATION CANNOT RENAME AN
--     AGGREGATE.

-- HISTORY:
--     JET 07/25/88  CREATED ORIGINAL TEST.

PROCEDURE B85008E IS

     TYPE REC IS RECORD
          A : INTEGER;
     END RECORD;

     EXCEPTION1 : EXCEPTION;

     EXCEPTION2 : EXCEPTION RENAMES EXCEPTION1;  -- OK.

     EXCEPTION5 : EXCEPTION RENAMES (A => 12);   -- ERROR: LITERAL

BEGIN
     NULL;
END B85008E;
