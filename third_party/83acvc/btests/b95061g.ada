-- B95061G.ADA
 
-- CHECK THAT AN ENTRY MAY NOT BE DECLARED TO RETURN A VALUE.
 
-- GLH  6/18/85
 
PROCEDURE B95061G IS
 
     TASK T IS

          ENTRY E1 (X : INTEGER) RETURN INTEGER;       -- ERROR: RETURN.

     END T;
 
     TASK BODY T IS
     BEGIN
          NULL;
     END T;
 
BEGIN
     NULL;
END B95061G;
