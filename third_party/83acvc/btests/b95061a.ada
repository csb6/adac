-- B95061A.ADA
 
-- CHECK THAT AN ARRAY TYPE DEFINITION IS NOT ALLOWED IN A FORMAL
-- PARAMETER DECLARATION OF AN ENTRY DECLARATION.
 
-- JWC 6/18/85
 
PROCEDURE B95061A IS
 
     TASK T IS
 
          ENTRY E9 (X: ARRAY(1..10) OF INTEGER);  -- ERROR: FORMAL ARRAY
                                                  -- TYPE DEFINITION.
     END T;
 
     TASK BODY T IS
     BEGIN
          NULL;
     END T;
 
BEGIN
     NULL;
END B95061A;
