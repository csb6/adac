-- CHECK THAT DECLARATIONS OTHER THAN ENTRY DECLARATIONS ARE NOT ALLOWED
-- IN TASK SPECIFICATIONS.

-- (J) TASK TYPE DECLARATION.

-- JRK 9/16/81
-- JBG 4/19/83

PROCEDURE B91002J IS

     TASK TYPE T2J IS

          ENTRY E;

          TASK TYPE T1;              -- ERROR: PROHIBITED IN TASK SPEC.

     END T2J;

     TASK BODY T2J IS
     BEGIN
          NULL;
     END;

BEGIN
     NULL;
END B91002J;
