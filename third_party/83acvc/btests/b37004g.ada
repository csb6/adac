-- B37004G.ADA

-- CHECK THAT ANONYMOUS RECORD COMPONENTS ARE FORBIDDEN.

-- DAT 5/18/81
-- ABW 6/10/82

PROCEDURE B37004G IS

     SUBTYPE S1 IS INTEGER RANGE 1 .. 1;

     TYPE REC IS RECORD
          C1 : ARRAY (S1) OF S1;        -- ERROR: ANONYMOUS TYPE
     END RECORD;

BEGIN
     NULL;

END B37004G;
