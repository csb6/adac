-- B85001G.ADA

-- OBJECTIVE:
--     CHECK THAT AN OBJECT RENAMING DECLARATION CANNOT RENAME A
--     SUBCOMPONENT OF A FUNCTION VALUE OR AN ATTRIBUTE WHICH RETURNS
--     A VALUE.

-- HISTORY:
--     JET 07/21/88  CREATED ORIGINAL TEST.

PROCEDURE B85001G IS
     TYPE REC0 IS RECORD
          F : INTEGER;
     END RECORD;

     TYPE REC IS RECORD
          I : INTEGER;
          B : BOOLEAN;
     END RECORD;

     FUNCTION FUNK RETURN REC;

     A : REC := (0, FALSE);
     B : INTEGER RENAMES A.I;           -- OK.
     T : INTEGER RENAMES FUNK.I.F;      -- ERROR: RENAMES SUBCOMPONENT
                                        --        OF FUNCTION CALL.
     U : INTEGER RENAMES INTEGER'FIRST; -- ERROR: RENAMES ATTRIBUTE.

     FUNCTION FUNK RETURN REC IS
     BEGIN
          RETURN (0, TRUE);
     END FUNK;

BEGIN
     NULL;
END B85001G;
